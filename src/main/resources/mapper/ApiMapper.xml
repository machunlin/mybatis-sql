<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.vipmro.athena.directory.service.mapper.ApiMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.vipmro.athena.directory.service.model.Api">
        <id column="id" property="id" />
        <result column="url_name" property="urlName" />
        <result column="url" property="url" />
        <result column="symbol" property="symbol" />
        <result column="app_id" property="appId" />
        <result column="status" property="status" />
        <result column="version" property="version" />
        <result column="create_at" property="createAt" />
        <result column="create_by" property="createBy" />
        <result column="create_by_name" property="createByName" />
        <result column="update_at" property="updateAt" />
        <result column="update_by" property="updateBy" />
        <result column="update_by_name" property="updateByName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, url_name AS urlName, url, symbol, app_id AS appId, status, version, create_at AS createAt, create_by AS createBy, create_by_name AS createByName, update_at AS updateAt, update_by AS updateBy, update_by_name AS updateByName
    </sql>
    <!-- 通用别名结果列 -->
    <sql id="Alias_Column_List">
        a.id,
        a.url_name AS urlName,
        a.url,
        a.symbol,
        a.app_id AS appId,
        a.status,
        a.version,
        a.create_at AS createAt,
        a.create_by AS createBy,
        a.create_by_name AS createByName,
        a.update_at AS updateAt,
        a.update_by AS updateBy,
        a.update_by_name AS updateByName,
        </sql>

</mapper>
