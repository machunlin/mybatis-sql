    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, goods_id AS goodsId, goods_code AS goodsCode, value, field, create_by AS createBy, create_by_name AS createByName, create_at AS createAt, update_at AS updateAt, update_by AS updateBy, update_by_name AS updateByName, status, version, trace_no AS traceNo
